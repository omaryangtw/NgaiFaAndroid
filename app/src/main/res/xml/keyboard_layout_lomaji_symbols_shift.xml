<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
          android:horizontalGap="@dimen/taigi_key_hgap"
          android:keyHeight="@dimen/taigi_key_height_symbol"
          android:keyWidth="@dimen/taigi_key_width"
          android:verticalGap="@dimen/taigi_key_vgap_symbol">

    <Row android:rowEdgeFlags="top">
        <Key
            android:codes="91"
            android:keyEdgeFlags="left"
            android:keyLabel="["/>
        <Key
            android:codes="93"
            android:keyLabel="]"/>
        <Key
            android:codes="123"
            android:keyLabel="{"/>
        <Key
            android:codes="125"
            android:keyLabel="}"/>
        <Key
            android:codes="35"
            android:keyLabel="\#"/>
        <Key
            android:codes="37"
            android:keyLabel="%"/>
        <Key
            android:codes="94"
            android:keyLabel="^"/>
        <Key
            android:codes="42"
            android:keyLabel="*"/>
        <Key
            android:codes="43"
            android:keyLabel="+"/>
        <Key
            android:codes="61"
            android:keyEdgeFlags="right"
            android:keyLabel="="/>
    </Row>

    <Row>
        <Key
            android:codes="95"
            android:keyEdgeFlags="left"
            android:keyLabel="_"/>
        <Key
            android:codes="92"
            android:keyLabel="\\"/>
        <Key
            android:codes="124"
            android:keyLabel="|"/>
        <Key
            android:codes="126"
            android:keyLabel="~"/>
        <Key
            android:codes="60"
            android:keyLabel="&lt;"/>
        <Key
            android:codes="62"
            android:keyLabel="&gt;"/>
        <Key
            android:codes="165"
            android:keyLabel="¥"/>
        <Key
            android:codes="8364"
            android:keyLabel="€"/>
        <Key
            android:codes="163"
            android:keyLabel="£"/>
        <Key
            android:codes="8226"
            android:keyEdgeFlags="right"
            android:keyLabel="•"/>
    </Row>

    <Row>
        <Key
                android:codes="-1"
                android:isModifier="true"
                android:isSticky="true"
                android:keyEdgeFlags="left"
                android:keyLabel="123"
                android:keyWidth="15%p"/>
        <Key
                android:codes="46"
                android:keyWidth="12.6%p"
                android:keyLabel="."/>
        <Key
                android:codes="44"
                android:keyWidth="12.6%p"
                android:keyLabel=","/>
        <Key
                android:codes="63"
                android:keyWidth="12.6%p"
                android:keyLabel="\?"/>
        <Key
                android:codes="33"
                android:keyWidth="12.6%p"
                android:keyLabel="!"/>
        <Key
                android:codes="39"
                android:keyWidth="12.6%p"
                android:keyLabel="'"/>
        <Key
            android:codes="-5"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_delete_lxx_dark"
            android:keyWidth="15%p"/>
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key
                android:codes="-2"
                android:keyEdgeFlags="left"
                android:keyLabel="羅馬字"
                android:keyWidth="15%p"/>
        <Key
                android:codes="-2003"
                android:keyLabel="T"
                android:keyWidth="10%p"/>
        <Key
                android:codes="32"
                android:isRepeatable="true"
                android:keyIcon="@drawable/btn_keyboard_spacebar_lxx_dark"
                android:keyWidth="46%p"/>
        <Key
                android:codes="10"
                android:keyEdgeFlags="right"
                android:keyIcon="@drawable/sym_keyboard_return"
                android:keyWidth="25%p"/>
    </Row>
</Keyboard>
